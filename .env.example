# ECLIPSEMOON AI Protocol Framework
# Environment Variables Example
# Author: ECLIPSEMOON

# Security
ENCRYPTION_KEY=your_encryption_key_here
KEY_STORAGE_PATH=~/.eclipsemoon/keys

# Network RPC Endpoints
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_WEBSOCKET_URL=wss://api.mainnet-beta.solana.com
ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/your_infura_key
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc

# API Keys
JUPITER_API_KEY=your_jupiter_api_key
RAYDIUM_API_KEY=your_raydium_api_key
DRIFT_API_KEY=your_drift_api_key
METEORA_API_KEY=your_meteora_api_key
MARGINFI_API_KEY=your_marginfi_api_key
KAMINO_API_KEY=your_kamino_api_key
LULO_API_KEY=your_lulo_api_key

# Wallet Configuration
WALLET_PRIVATE_KEY=your_encrypted_private_key
WALLET_PUBLIC_KEY=your_public_key
WALLET_MNEMONIC=your_encrypted_mnemonic

# Data Providers
COINGECKO_API_KEY=your_coingecko_api_key
BINANCE_API_KEY=your_binance_api_key
BINANCE_API_SECRET=your_binance_api_secret
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key

# AI Models
OPENAI_API_KEY=your_openai_api_key
HUGGINGFACE_API_KEY=your_huggingface_api_key
MODEL_CACHE_DIR=~/.eclipsemoon/models

# Telemetry
TELEMETRY_ENABLED=false
TELEMETRY_ENDPOINT=https://telemetry.eclipsemoon.ai/ingest
TELEMETRY_API_KEY=your_telemetry_api_key

# Logging
LOG_LEVEL=INFO
LOG_FILE_PATH=logs/eclipsemoon.log
LOG_ROTATION=daily
LOG_MAX_FILES=30

# Performance
CACHE_ENABLED=true
CACHE_TTL=3600
PARALLEL_REQUESTS=5
REQUEST_TIMEOUT=30

# Risk Management
MAX_POSITION_SIZE_USD=10000
MAX_LEVERAGE=10
AUTO_CLOSE_THRESHOLD=0.8
STOP_LOSS_DEFAULT=0.05
TAKE_PROFIT_DEFAULT=0.1

# Strategy Configuration
DEFAULT_STRATEGY_PATH=config/strategies/default.yaml
STRATEGY_AUTO_RESTART=true
BACKTEST_DATA_PATH=data/historical

# Notification
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id
EMAIL_NOTIFICATIONS_ENABLED=false
EMAIL_SMTP_SERVER=smtp.example.com
EMAIL_SMTP_PORT=587
EMAIL_USERNAME=your_email@example.com
EMAIL_PASSWORD=your_email_password
EMAIL_RECIPIENT=notifications@example.com

console.log("This is a Node.js representation of the .env.example file. In a real implementation, this would be a plain text file.");